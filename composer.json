{
    "name": "bushbaby/flysystem",
    "description": "Zend Framework 2 module bridge for flysystem filesystem.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bas Kamer",
            "email": "bas@bushbaby.nl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "league/flysystem": "~1.0",
        "zendframework/zend-servicemanager": "~2.4",
        "zendframework/zend-cache": "~2.2",
        "zendframework/zend-stdlib": "~2.2|~3.0",
        "zendframework/zend-modulemanager": "~2.2"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~2.0",
        "phpunit/phpunit": "~4.5",
        "ocramius/proxy-manager": "~1.0",
        "league/flysystem-eventable-filesystem": "~1.0",
        "league/flysystem-rackspace": "~1.0",
        "league/flysystem-replicate-adapter": "~1.0",
        "league/flysystem-copy": "~1.0",
        "league/flysystem-webdav": "~1.0",
        "league/flysystem-dropbox": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "league/flysystem-sftp": "~1.0",
        "league/flysystem-ziparchive": "~1.0",
        "league/flysystem-azure": "~1.0,>=1.0.2",
        "zendframework/zend-config": "~2.2",
        "zendframework/zend-filter": "~2.2",
        "zendframework/zend-i18n": "~2.2",
        "zendframework/zend-loader": "~2.2",
        "zendframework/zend-log": "~2.2",
        "zendframework/zend-mvc": "~2.2",
        "zendframework/zend-serializer": "~2.2",
        "zendframework/zend-view": "~2.2",
        "league/flysystem-vfs": "^1.0",
        "pear-pear.php.net/http_request2" : ">=2.3"
    },
    "suggest": {
        "zendframework/zend-filter": "zend-filter component ~2.2, if you need RenameUpload filter"
    },
    "autoload": {
        "psr-4": {
            "BsbFlysystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BsbFlysystemTest\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4.x-dev"
        }
    },
    "repositories": [
      {
        "type": "pear",
        "url": "https://pear.php.net"
      }
    ]
}
